<h1>Product List</h1>
<table border="1">
    <tr>
        <th>Name</th>
        <th>Price</th>
        <th>Image</th>
        <th>Category</th>
        <th>Menu</th>
    </tr>
    {{#each productList }}        
    <tr>
        <td>{{name}}</td>
        <td>{{price}}</td>
        <td>
            <img src="{{image}}" alt="" height="200">
        </td>
        <td>{{category.name}}</td>
        <td>
            
            <a href="/products/edit/{{ _id }}">Edit</a>
            <a href="">Delete</a>
            <a href="/products/delete/{{_id}}"          onclick="return confirm('Do you want to delete this product ?')" ;>Delete</a>
        </td>
    </tr>
    {{/each}}
</table>